{
  "name": "angular-meta-trader",
  "version": "0.0.1",
  "description": "Angular UI with NGRX, SSR and WebSocket for Meta Trader forex terminal",
  "scripts": {
    "add-socket": "node scripts/socket-env.mjs",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "dev:ssr": "ng run angular-meta-trader:serve-ssr",
    "serve:ssr": "node dist/angular-meta-trader/server/main.js",
    "serve:ssr:inspect": "node --inspect dist/angular-meta-trader/server/main.js",
    "build:ssr": "ng build && ng run angular-meta-trader:server",
    "build:ssr:only": "ng run angular-meta-trader:server",
    "prerender": "ng run angular-meta-trader:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.0.0",
    "@angular/common": "^16.0.0",
    "@angular/compiler": "^16.0.0",
    "@angular/core": "^16.0.0",
    "@angular/forms": "^16.0.0",
    "@angular/platform-browser": "^16.0.0",
    "@angular/platform-browser-dynamic": "^16.0.0",
    "@angular/platform-server": "^16.0.0",
    "@angular/router": "^16.0.0",
    "@angular/service-worker": "^16.0.0",
    "@fortawesome/angular-fontawesome": "^0.13.0",
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@ngrx/effects": "^16.0.0",
    "@ngrx/entity": "^16.0.0",
    "@ngrx/eslint-plugin": "^16.0.0",
    "@ngrx/store": "^16.0.0",
    "@ngrx/store-devtools": "^16.0.0",
    "@nguniversal/express-engine": "^16.0.1",
    "@popperjs/core": "^2.11.7",
    "bootstrap": "^5.2.3",
    "echarts": "^5.4.2",
    "express": "^4.15.2",
    "ngx-echarts": "^16.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "ws": "^8.13.0",
    "zone.js": "~0.13.0"
  },
