{
  "name": "angular-meta-trader",
  "version": "0.0.1",
  "description": "Angular UI with NGRX, SSR and WebSocket for Meta Trader forex terminal",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "dev:ssr": "ng run angular-meta-trader:serve-ssr",
    "serve:ssr": "node dist/angular-meta-trader/server/main.js",
    "serve:ssr:inspect": "node --inspect dist/angular-meta-trader/server/main.js",
    "build:ssr": "ng build && ng run angular-meta-trader:server",
    "build:ssr:only": "ng run angular-meta-trader:server",
    "prerender": "ng run angular-meta-trader:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.3.11",
    "@angular/common": "~13.3.11",
    "@angular/compiler": "~13.3.11",
    "@angular/core": "~13.3.11",
    "@angular/forms": "~13.3.11",
    "@angular/platform-browser": "~13.3.11",
    "@angular/platform-browser-dynamic": "~13.3.11",
    "@angular/platform-server": "~13.3.11",
    "@angular/router": "~13.3.11",
    "@angular/service-worker": "~13.3.11",
    "@fortawesome/angular-fontawesome": "^0.10.2",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@ngrx/effects": "^13.2.0",
    "@ngrx/entity": "^13.2.0",
    "@ngrx/store": "^13.2.0",
    "@ngrx/store-devtools": "^13.2.0",
    "@nguniversal/express-engine": "^13.1.1",
    "@popperjs/core": "^2.11.5",
    "bootstrap": "^5.1.3",
    "echarts": "^5.3.2",
    "express": "^4.15.2",
    "ngx-echarts": "^8.0.1",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "ws": "^8.8.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/jest": "latest",
    "@angular-devkit/build-angular": "~13.3.8",
    "@angular-eslint/builder": "13.2.1",
    "@angular-eslint/eslint-plugin": "13.2.1",
    "@angular-eslint/eslint-plugin-template": "13.2.1",
    "@angular-eslint/schematics": "13.2.1",
    "@angular-eslint/template-parser": "13.2.1",
    "@angular/cli": "~13.3.8",
    "@angular/compiler-cli": "~13.3.11",
    "@briebug/jest-schematic": "^4.0.0",
    "@cypress/schematic": "^1.7.0",
    "@ngrx/schematics": "^13.2.0",
    "@nguniversal/builders": "^13.1.1",
    "@types/echarts": "^4.9.15",
    "@types/express": "^4.17.0",
    "@types/jest": "latest",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "5.17.0",
    "@typescript-eslint/parser": "5.17.0",
    "cypress": "latest",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-ngrx": "^2.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "latest",
    "prettier": "^2.6.2",
    "typescript": "~4.6.2"
  }
}
